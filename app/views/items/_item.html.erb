<div id="<%= dom_id item %>">
  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= item.price %>
  </p>

  <p>
    <strong>Callories:</strong>
    <%= item.callories %>
  </p>

  <p>
    <strong>Dish:</strong>
    <%= item.dish_id %>
  </p>

  <p>
    <strong>Restaurant:</strong>
    <%= item.restaurant_id %>
  </p>

</div>
